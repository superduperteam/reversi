<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>SignupServlet</servlet-name>
        <servlet-class>game.servlets.signup.SignupServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>game.servlets.lobby.LogoutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>XmlFileLoaderServlet</servlet-name>
        <servlet-class>game.servlets.lobby.XmlFileLoaderServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>OnlinePlayersServlet</servlet-name>
        <servlet-class>game.servlets.lobby.OnlinePlayersServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewRoomServlet</servlet-name>
        <servlet-class>game.servlets.lobby.NewRoomServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AvailableRoomsServlet</servlet-name>
        <servlet-class>game.servlets.lobby.AvailableRoomsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>JoinRoomServlet</servlet-name>
        <servlet-class>game.servlets.lobby.JoinRoomServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ChatServlet</servlet-name>
        <servlet-class>game.servlets.room.gameStart.ChatServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SendChatServlet</servlet-name>
        <servlet-class>game.servlets.room.gameStart.SendChatServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RoomJoinedPlayersServlet</servlet-name>
        <servlet-class>game.servlets.room.preGameStart.RoomJoinedPlayersServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LeaveRoomServlet</servlet-name>
        <servlet-class>game.servlets.room.preGameStart.LeaveRoomServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InitializeGameServlet</servlet-name>
        <servlet-class>game.servlets.room.gameStart.InitializeGameServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ThisPlayerServlet</servlet-name>
        <servlet-class>game.servlets.room.gameStart.ThisPlayerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CurrentPlayerTurnServlet</servlet-name>
        <servlet-class>game.servlets.room.gameStart.CurrentPlayerTurnServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ExecuteMoveServlet</servlet-name>
        <servlet-class>game.servlets.room.gameStart.ExecuteMoveServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BoardUpdaterServlet</servlet-name>
        <servlet-class>game.servlets.room.gameStart.BoardUpdaterServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GameOverServlet</servlet-name>
        <servlet-class>game.servlets.room.gameStart.GameOverServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EndGameLeaveServlet</servlet-name>
        <servlet-class>game.servlets.room.gameStart.EndGameLeaveServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SynchronizeEndTurnServlet</servlet-name>
        <servlet-class>game.servlets.room.gameStart.SynchronizeEndTurnServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PlayerQuitServlet</servlet-name>
        <servlet-class>game.servlets.room.gameStart.PlayerQuitServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SignupServlet</servlet-name>
        <url-pattern>/signup</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ChatServlet</servlet-name>
        <url-pattern>/chat</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SendChatServlet</servlet-name>
        <url-pattern>/sendChat</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>XmlFileLoaderServlet</servlet-name>
        <url-pattern>/xmlFileLoader</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>OnlinePlayersServlet</servlet-name>
        <url-pattern>/whosOnline</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewRoomServlet</servlet-name>
        <url-pattern>/newRoom</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AvailableRoomsServlet</servlet-name>
        <url-pattern>/availableRooms</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>JoinRoomServlet</servlet-name>
        <url-pattern>/joinRoom</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RoomJoinedPlayersServlet</servlet-name>
        <url-pattern>/roomJoinedPlayers</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LeaveRoomServlet</servlet-name>
        <url-pattern>/leaveRoom</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InitializeGameServlet</servlet-name>
        <url-pattern>/initializeGame</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ThisPlayerServlet</servlet-name>
        <url-pattern>/thisPlayer</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CurrentPlayerTurnServlet</servlet-name>
        <url-pattern>/currentPlayerTurn</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ExecuteMoveServlet</servlet-name>
        <url-pattern>/executeMove</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BoardUpdaterServlet</servlet-name>
        <url-pattern>/boardUpdater</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GameOverServlet</servlet-name>
        <url-pattern>/gameOver</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EndGameLeaveServlet</servlet-name>
        <url-pattern>/endGameLeave</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SynchronizeEndTurnServlet</servlet-name>
        <url-pattern>/synchronizeEndTurn</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PlayerQuitServlet</servlet-name>
        <url-pattern>/playerQuit</url-pattern>
    </servlet-mapping>
</web-app>